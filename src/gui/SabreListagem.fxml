<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19">
   <children>
      <Label text="Listagem dos Sabres">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView>
        <columns>
          <TableColumn prefWidth="64.0" text="ID" />
            <TableColumn prefWidth="272.0" text="JEDI" />
            <TableColumn prefWidth="90.0" text="FABRICACAO" />
            <TableColumn prefWidth="75.0" text="SITUACAO" />
          <TableColumn prefWidth="77.0" text="MODELO" />
            <TableColumn prefWidth="57.0" text="BOTAO" />
            <TableColumn prefWidth="92.0" text="GALAXIA" />
            <TableColumn prefWidth="69.0" text="SENSOR" />
            <TableColumn prefWidth="72.0" text="VALIDADE" />
        </columns>
      </TableView>
   </children>
</VBox>
